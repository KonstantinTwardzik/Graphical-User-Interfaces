<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<GridPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.plusminus.PlusMinusController">
   <children>
      <Button text="+" fx:id="plus" onAction="#buttonClickedPlus" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label text="0" fx:id="counterL" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Button text="-" fx:id="minus" onAction="#buttonClickedMinus" GridPane.columnIndex="1" GridPane.rowIndex="3" />
   </children>
</GridPane>
